@model IEnumerable<StudentManagementApp.Models.StudentAttendance>

<div class="row">
    <div class="col-9">
        <h2 class="text-primary">Student Attendance List</h2>
    </div>
    <div class="col-3">
        <a class="btn btn-success form-control" asp-action="SaveOrUpdate">
            <i class="fas fa-plus"></i>
            Add Attendance
        </a>
    </div>
</div>
<div class="container rounded border table-responsive m-2 p-2">
    <table id="tabledata" class="table table-bordered table-active table-striped">
        <thead>
            <tr>
                <th>Class</th>
                <th>Subject</th>
                <th>Roll No</th>
                <th>Status</th>
                <th>Date</th>
                <th>Action Buttons</th>
            </tr>
        </thead>
    </table>
</div>

@section scripts {
    <script src="~/js/StudentAttendance.js"></script>
}
