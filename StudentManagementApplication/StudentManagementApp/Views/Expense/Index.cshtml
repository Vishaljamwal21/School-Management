@model IEnumerable<StudentManagementApp.Models.Expense>

<div class="row">
    <div class="col-9">
        <h2 class="text-primary">Expenses</h2>
    </div>
    <div class="col-3">
        <a class="btn btn-success form-control" asp-action="SaveorUpdate">
            <i class="fas fa-plus"></i>
            Add Expense
        </a>
    </div>
</div>
<div class="container rounded border table-responsive m-2 p-2">
    <table id="tabledata" class="table table-bordered table-active table-striped">
        <thead>
            <tr>
                <th>Class</th>
                <th>Subject</th>
                <th>Charge Amount</th>
                <th>Action Buttons</th>
            </tr>
        </thead>      
    </table>
</div>

@section Scripts {
    <script src="~/js/Expense.js"></script>
}
